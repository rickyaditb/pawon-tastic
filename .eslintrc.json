{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": [
            "error",
            {"devDependencies": true}
          ],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "class-methods-use-this": "off",
        "no-console": "off"
    }
}
